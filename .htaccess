RewriteRule ^myAllPhotos$ myAllPhotos.php [L]
RewriteRule ^editProfile$ editProfile.php [L]
RewriteRule ^login$ login.php [L]
RewriteRule ^register$ register.php [L]
RewriteRule ^publicPhotos$ publicPhotos.php [L]
